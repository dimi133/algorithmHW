// b. Рассмотрите задачу динамического увеличения массива. Реализуйте функцию, которая принимает на вход массив и число элементов, 
      и возвращает новый массив, который содержит все элементы исходного массива и заданное число новых элементов.

function dinamicArr(arr, num) {
    let newArr = arr.slice();

    for (let i=0; i<num; i++) {
        newArr.push("New Element" + (i+1));
    }
    return newArr
}

dinamicArr(["Elem 1", "Elem 2", "Elem 3", "Elem 4"], 3);
console.log(dinamicArr);
